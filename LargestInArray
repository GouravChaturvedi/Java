import java.util.*;
public class Arrays {
    public static int LargestInArray(int numbers[]){
        int max=Integer.MIN_VALUE;
        for(int i=0;i<numbers.length;i++){
           if(numbers[i]>max){
               max=numbers[i];
           }
        }
        return max;
    }
    public static void main(String[] args) {
        int numbers[]={1,2,5,7,4,9,6};
        System.out.println("The largest number is: " + LargestInArray(numbers));
    }
}

import java.util.*;
public class Arrays {
    public static int LinearSearch(int numbers[],int key){
        for(int i=0;i<numbers.length;i++){
            if(numbers[i]==key){
                return i;
            }
        }
        return -1;
    }
    public static void main(String[] args) {
        int numbers[]={2,3,5,6,7,8,9};
        int key=8;
        System.out.println("Number is found at Index: " + LinearSearch(numbers,key));
    }
}
